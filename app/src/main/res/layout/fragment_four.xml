<?xml version="1.0" encoding="utf-8"?><!--我的页面的布局-->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray"
    android:orientation="vertical"
    android:paddingBottom="49dp">

    <RelativeLayout
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="195dp">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="match_parent"
            android:layout_height="195dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@mipmap/wodebeijing" />

        <RelativeLayout
            android:id="@+id/mine_iv_desgin"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_marginTop="@dimen/y15">

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerInParent="true"
                android:src="@mipmap/shezhi" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/mine_iv_message"
            android:layout_width="40dp"
            android:layout_height="50dp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/y15">

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerInParent="true"
                android:background="@mipmap/xiaoxi_n" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/mine_iv_head"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:src="@mipmap/weidenglu" />

            <LinearLayout
                android:id="@+id/mine_ll_login"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="20dp"
                android:layout_toRightOf="@id/mine_iv_head"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/mine_tv_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="@string/mine_tv_name"
                    android:textColor="@color/black"
                    android:textSize="16sp" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/mine_iv_grade"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:background="@mipmap/huiyuan" />

                    <!--<RelativeLayout-->
                    <!--android:id="@+id/mine_iv_up"-->
                    <!--android:layout_width="40dp"-->
                    <!--android:layout_height="20dp"-->
                    <!--android:layout_marginLeft="2dp"-->
                    <!--android:layout_toRightOf="@id/mine_iv_grade">-->

                    <!--<ImageView-->
                    <!--android:layout_width="30dp"-->
                    <!--android:layout_height="15dp"-->
                    <!--android:background="@mipmap/shengji" />-->

                    <!--</RelativeLayout>-->


                </LinearLayout>

            </LinearLayout>

            <RelativeLayout
                android:id="@+id/mine_iv_go"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_alignParentRight="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp">

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:layout_centerInParent="true"
                    android:background="@mipmap/go" />

            </RelativeLayout>

            <ImageView
                android:id="@+id/mine_iv_code"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:layout_marginRight="10dp"
                android:layout_toLeftOf="@id/mine_iv_go" />

        </RelativeLayout>

    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y343"
        android:layout_below="@id/top"
        android:background="@mipmap/beijing"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginRight="@dimen/x33"
            android:layout_weight="1"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/mine_iv_setting"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x80"
                    android:layout_height="@dimen/y80"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:src="@mipmap/mine_iv_personal" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:text="@string/mine_tv_personal" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/mine_iv_my_older"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginRight="@dimen/x15"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x80"
                    android:layout_height="@dimen/y80"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:src="@mipmap/older" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:text="@string/my_older" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/mine_iv_address"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x80"
                    android:layout_height="@dimen/y80"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:src="@mipmap/address" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:text="@string/my_address" />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/mine_iv_collection"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginLeft="@dimen/x40"
            android:layout_marginRight="@dimen/x33"
            android:layout_weight="1"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x80"
                    android:layout_height="@dimen/y80"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:src="@mipmap/collection" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:text="@string/my_collection" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/mine_iv_call"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_marginRight="@dimen/x15"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x80"
                    android:layout_height="@dimen/y80"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:src="@mipmap/call" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:text="@string/call" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/mine_iv_zhaungxiu"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x80"
                    android:layout_height="@dimen/y80"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:src="@mipmap/zhaungxiu" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/y10"
                    android:gravity="center"
                    android:text="@string/zhuangxiu" />
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>


</RelativeLayout>